   <!--最外层box -->

 <div class="wrapper-box " >

        <div class="swiper-container fav-swiper-container" id="swiper-container2">

            <div class="swiper-wrapper">

                <div class="swiper-slide wrapper1">



                            <!-- ==瀑布流== -->
                            <div class="waterfall1 water">

                      

                                <script type="text/html" id="template1">
                                 
                                  {{if data==""}}

                                       <div class="no-favorite" >
                                           
                                              <p>你暂时没有收藏，快去心愿狂狂吧</p>

                                              <div class="no-favorite-bg ico-no-fav"></div>

                                       </div>

                                  {{else}}


                                             {{each data as value}}

                                                    <div class="picList" onclick="favoriteDetail('{{value.id}}','{{value.type}}')">
                                                        <div class="picList-box">
                                                            <div class="picThumbnail">

                                                                <img  class="lazy-img"   data-original="{{value.icon}}"  width="100%" height="{{value.h}}" style="height:{{value.h}}px;" />
                                                                
                                                              
                                                            </div>

                                                            <div class="picDescription">

                                                                <p class="picDescription-title">
                                                                     {{if value.isRent==1}}
                                                                         
                                                                        <span class="isRent">可租</span>

                                                                      {{/if}}


                                                                       <i>{{value.title}}</i>

                                                                </p>

                                                                <p class="picDescription-money">
                                                                     &yen;<span class="fav-price">{{value.price}}</span>
                                                                  
                                                                    {{if value.isPreference==1}}
                                                                     
                                                                      <i class="isSign">{{value.activityName}}</i>

                                                                    {{/if}}

                                                                </p>
                                                                <ul class=" iconfont-ul fav-operate">
                                                                    <li>
                                                                        <a>
                                                                            <i class="iconfont eye">&#xe605;</i>
                                                                            <span>{{value.viewNum}}</span>
                                                                        </a>

                                                                    </li>
                                                                    <li onclick="removeFav(this,event,'{{value.id}}','{{value.type}}')">
                                                                        <a>
                                                                                <i class="iconfont star">
                                                                                &#xe62e;</i><span>{{value.keepNum}}</span>

                                                                        </a>
                                                                    </li>

                                                                    <li class="last-li">
                                                                        <a>
                                                                            <i class="iconfont love">
                                                                                &#xe652;</i><span>{{value.likeNum}}</span>
                                                                        </a>
                                                                    </li>

                                                                </ul>

                                                            </div>

                                                        </div>

                                                    </div>


                                        {{/each}}



                                  {{/if}}

                              


                                </script>

                            </div>


                   


                </div>


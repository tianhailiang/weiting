/*婚礼堂套餐*/
.packge-tab {
    margin-top: 20px;
    background: #f8f8f8;
}
.packge-tab > ul {
    display: -webkit-box;
    display: box;
    box-pack: justify;
}
.packge-tab > ul > li {
    -webkit-border-radius: 5px 5px 0 0;
    border-radius: 5px 5px 0 0;
    color: white;
    height: 40px;
    background: #7a7b87;
    -webkit-box-flex: 1;
    box-flex: 1;
    font-size: 12px;
}
.packge-tab > ul > li.selected {
    background: #e8796e;
}
.packge-tab > ul li p {
    font-size: 14px;
    padding-top: 2px;
}
.w-33 {
    width: 33%;
}
.w-66 {
    width: 66.4%;
}
.w-66 > ul > li {
    height: 38px;
    line-height: 38px;
    border-bottom: 1px solid #e1e1e1;
    border-right: 1px solid #e1e1e1;
}
.w-66 > ul > li a {
    display: inline-block;
    font-size: 12px;
    text-align: center;
    width:61px;
    height:18px;
    line-height:18px;
    color: #e8796e;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    border: 1px solid #e8796e;
    margin-right: 10px;
}
.w-66 > ul > li span:nth-child(1) {
    padding-left: .7rem;
    padding-right: 57px;
    font-size: .26rem;
}
.w-66 > ul > li span:nth-child(2) {
    line-height: 36px;
    -webkit-border-radius: 6px;
    border-radius: 6px;

}

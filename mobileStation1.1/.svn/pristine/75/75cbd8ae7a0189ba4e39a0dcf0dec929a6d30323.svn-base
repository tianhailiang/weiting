

/*=====personal.html=======*/

.mic-top{
	width:100%;
	height:173px;
	
	display:flex;
   
    flex-direction:column;
 
     align-items:center;
     padding-top:38px;
     box-sizing: border-box;
     background:#fff;

}


.mic-header-box{

  	display:-webkit-box;
	display:-webkit-flex;
	display:-moz-box;
	display:-ms-flexbox;
	display:flex;
    -webkit-box-orient:vertical;
    -webkit-box-direction:normal;
    -webkit-flex-direction:column;
       -moz-box-orient:vertical;
       -moz-box-direction:normal;
        -ms-flex-direction:column;
            flex-direction:column;
   -webkit-box-align:center;
   -webkit-align-items:center;
      -moz-box-align:center;
       -ms-flex-align:center;
           align-items:center;

}


.mic-header{

	width:74px;
	height:74px;
	-webkit-border-radius: 50%;
	 border-radius: 50%;
	
}

.mic-name{
	margin-top:12.5px;
	font-size:15px;
	color:#333;
    line-height:15px;

}

.mic-name span{
	font-weight: bold;
}

.mic-name i{
	margin-left: 10px;
	color:#333;
}


.mic-ul{
	width:100%;
	margin-top: 10px;
	background:#fff;
} 

.mic-ul li{
  width:100%;
  
   
   display: flex;

   justify-content:space-between;

   align-items:center;
   padding-left:22px;
   padding-right:14px;
   position:relative;
   height:57px;
  
   box-sizing: border-box;

}



.mic-ul li:after{
	content:"";
	position:absolute;
	left:0;
	bottom:0;
	width:100%;
	height:1px;
	background-color:#e5e5e5;

	-webkit-transform:scaleY(0.5);
	 -moz-transform:scaleY(0.5);
	  -ms-transform:scaleY(0.5);
	     transform:scaleY(0.5);
	  

	-webkit-transform-origin:0 0;
		-moz-transform-origin:0 0;
		 -ms-transform-origin:0 0;
	     transform-origin:0 0;


}

.mic-ul li div{
	
	display: flex;
	
	align-items:center;
	font-size:14px;
	color:#333;

}

.mic-li-left i{

	display: block;
	width:30px;
	height:30px;
	margin-right: 19px;
}





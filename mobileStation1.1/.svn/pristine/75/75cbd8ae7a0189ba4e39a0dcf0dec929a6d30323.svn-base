/*======myactivities.html========*/

.act_ul{
	margin-top: 10px;
}

.act_ul dt{
	position: relative;
}


.mic_act_top{
	width: 100%;
    height: 35px;
    padding-top: 15px;
    font-size: 14px;
    font-weight: bold;
    color: #333;
    box-sizing: border-box;
    line-height: 14px;
}

.act_come{
	height:15px;
}

.act_endtime{
	font-size: 11px;
	color:#999;
	line-height: 11px;
}

.mic_act_bot{
	font-size: 14px;
	color:#666;
	line-height: 22.5px;
	padding-bottom: 5px;
}


.act_ul dt:nth-of-type(1) .pastDue{
   
   top:0;
}

.act_ul dt:nth-of-type(1) .past-sign{

	top:4px;
}


.pastDue{
	width:100%;
	height:100%;
	position:absolute;
	left:0;
	top:15px;

	opacity:0.33;
	background-color:#000; 
	box-sizing: border-box;
}


.past-sign{
	
	width:75.5px;
	height:75.5px;
	position:absolute;
	right:2.5px;
	top:19px;
	
	z-index:150;
	
	
	
}



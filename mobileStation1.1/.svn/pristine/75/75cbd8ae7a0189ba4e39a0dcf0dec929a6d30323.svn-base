/*===============mygift.html========================*/


.mic_gift_ul{
	position: fixed;
	top:44px;

	width:100%;
	height:34px;
	background:#fff;
	display: flex;

	border-top: 1px solid #d9dbdc;
	

}




.mic_gift_ul li{

	flex:1;
	display: flex;
	justify-content:center;
	align-items:center;
}

.mic_gift_ul li span{
	
	height:100%;
	display: flex;
    align-items:center;
    color:#666;
    font-size: 14px;

}


.mic_gift_ul li span.gift_active{

	color:#e8796e;
	
}


.gift_box{
	position: absolute;
	width: 100%;
	-webkit-overflow-scrolling : touch;
	top:78px;
}


.gift_slide_ul{
	width:100%;
	padding:0 12.5px;
	box-sizing: border-box;
}

.gift_slide_ul li{
	width:100%;
	margin-top: 10px;
	background:#fff;
	position: relative;
}

.gift_slide_ul li img{

	width:100%;
	height:127px;
}


.gift_rule{

	width:100%;
	box-sizing: border-box;
	padding:0 15px;

}

.gift_date{
	display: block;
	padding-top: 12px;
	font-size: 11px;
	line-height: 11px;
	color:#999;
}

.gift_rule_p{
	color:#999;
	font-size: 11px;
	line-height: 17px;
    margin-top: 5px;
   padding-bottom: 5px;

}


.gift_indicate{
	 position: absolute;

    width:100%;
    height:1px;
   
      -webkit-box-sizing: border-box;

    -moz-box-sizing: border-box;

    box-sizing: border-box;
    bottom: 0;
   background:#d9dbdc;
}


.gift_ibox{

    
    height:1px;
    background:#e8796e;
    width:14.4%;   
    margin-left: 17%;
    -webkit-transition: -webkit-transform 0.8s;
    transition: -webkit-transform 0.8s;
    -moz-transition: transform 0.8s, -moz-transform 0.8s;
    transition: transform 0.8s;
    transition: transform 0.8s, -webkit-transform 0.8s, -moz-transform 0.8s;

}


.gift_pastDue{
	  width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    opacity: 0.33;
    background-color: #000;
    box-sizing: border-box;
}
<body style="background:#f2f4f5;">

  <header  class="header_box top_fixed" style="background:#fff;">
        <ul>
            <li class="first" >
              
            </li>

          <li class="second">
           

          </li>
          
          <li class="last">

            
            <span class="icon-bell-box">

               <i class="iconfont icon-bell" id="bell">&#xe618;</i>
               <i class="icon-red-circle hide" id="news"></i>

            </span>

               <i class="iconfont icon-service" id="service" >&#xe70d;</i>
            
          </li>

        
        </ul>
        
</header>

<div class="mic-top">

    <div id="mic-header-box" class="mic-header-box" onclick="perInfomation()">
                <div class="mic-header" id="mic-header"></div>
                <a class="mic-name" ><span id="micUserName"></span><i class="iconfont">&#xe621;</i></a>

    </div>
</div>

<ul class="mic-ul">


    <li onclick="myactivity()">

        <div class="mic-li-left">
             
            <i class="ico_mic_activty"></i>

            <span>我的活动</span>

        </div>
        <div class="mic-li-right">
        
            <i class="iconfont advance">&#xe65b;</i>
        </div>
    </li>

    <li onclick="myOrder()">
        <div class="mic-li-left">

            <i class="ico_mic_order"></i>
            <span >我的预约</span>

        </div>
        <div class="mic-li-right">
            <i class="iconfont advance">&#xe65b;</i>
        </div>
    </li>

    <li  onclick="myfavorite()">
        <div class="mic-li-left">
            
            <i class="ico_mic_collect" ></i>

            <span >我的收藏</span>

        </div>

        <div class="mic-li-right">
           
            <i class="iconfont advance">&#xe65b;</i>
        </div>

    </li>


    <li onclick="myCoupon()">

        <div class="mic-li-left">
            
            <i class="ico_mic_gift"></i>
            <span >我的礼券</span>

        </div>

        <div class="mic-li-right">

            <i class="iconfont advance">&#xe65b;</i>
        </div>

    </li>
</ul>


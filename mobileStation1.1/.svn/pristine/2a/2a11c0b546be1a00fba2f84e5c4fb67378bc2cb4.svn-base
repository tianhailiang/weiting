/*====这儿是footer=====*/
.ico-wish, .ico-list, .ico-order, .ico-miccenter, .ico-current-wish, .ico-current-list, .ico-current-order, .ico-current-miccenter {
    -webkit-background-size: 25px 225px;
    background-size: 25px 225px;
    background-image: url(../images/common_0.5_z.png)
}
.ico-wish {
    background:url(../images/wish_0.png?__sprite) no-repeat 0 0;
}

.ico-list {
    background:url(../images/bill_0.png?__sprite) no-repeat 0 0;
}

.ico-miccenter {
    background:url(../images/mine_0.png?__sprite) no-repeat 0 0;
}


.ico-current-wish {
    background:url(../images/wish_1.png?__sprite) no-repeat 0 -48.5px;
}

.ico-current-list {
    background:url(../images/bill_1.png?__sprite) no-repeat 0 0;
}

.ico-current-miccenter {
    background:url(../images/mine_1.png?__sprite) no-repeat 0 0;
}
.footer-box {
    width: 100%;
    height: 49px;
    position: relative;
    z-index: 99;


}
.footer-box nav {
    width: 100%;
    height: 100%
}
.footer-box ul {
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
}
.footer-box ul li {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -moz-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    height: 100%;

    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    -moz-box-orient: vertical;
    -moz-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -moz-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -moz-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}
.footer-box ul li a {
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    -moz-box-orient: vertical;
    -moz-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -moz-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -moz-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}
.footer-box ul li i {
    font-size: 11px;
    color: #7a7b87;
    margin-top: 3.5px;
}
.footer-box ul li span {
    width: 25px;
    height: 25px;
}

.footer-fixed {
    position: fixed !important;
    bottom: 0;
    z-index: 12;
    background-color: white;
}

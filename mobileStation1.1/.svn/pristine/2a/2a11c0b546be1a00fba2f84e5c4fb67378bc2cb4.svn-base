/*header 头部样式*/
.fixed-header{
    height: 44px;
    line-height: 44px;
    overflow: hidden;
}
.filter-header {
    line-height: 44px;
    overflow: hidden;
    left:15px;
}
.filter-header i img{
    width:7px;
    height: 7px;
    margin-left: 5px;
}
.logo{
    width:100px;
    height:40px;
    margin:0 auto;
    top:14px;
}
.info-notice{
    right:15px;
    top: 4px;
}
.info-notice span:nth-child(1){
    width:13px;
    height:16px;
    margin-right: 15px;
}
.info-notice span:nth-child(2){
    width:16px;
    height:18px;
}
.infor-tip{
    display: inline-block;
    width:6px;
    height:6px;
    background: #e8796e;
    border-radius: 50%;
    top: 8px;
    left: 12px;
}
.black-layer {
    background: #1f1f1f;
    opacity: 0.7;
    width: 100%;
    top: 40px;
    z-index: 23;
}
.type {
    left: 0;
    top: 0;
    background: white;
    z-index: 9;
    width: 100%;
}
.type li {
    height: 40px;
    line-height: 40px;
    border-bottom: 1px solid #e1e1e1;
    color: #7a7b87;
    text-align: center;
    font-size: 14px;
}
.city-selct-box{
    width:100%;
    z-index: 25;
}
